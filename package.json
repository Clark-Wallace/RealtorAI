{
  "name": "realtor-ai",
  "version": "2.0.0",
  "description": "A comprehensive real estate intelligence platform combining client preference tracking with powerful market analytics",
  "author": "RealtorAI Contributors",
  "license": "MIT",
  "homepage": "https://github.com/Clark-Wallace/RealtorAI#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Clark-Wallace/RealtorAI.git"
  },
  "bugs": {
    "url": "https://github.com/Clark-Wallace/RealtorAI/issues"
  },
  "keywords": [
    "real-estate",
    "realtor",
    "property-management",
    "market-intelligence",
    "client-tracking",
    "pwa",
    "react",
    "vite"
  ],
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "deploy": "./scripts/deploy.sh",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:prod": "vite preview --mode production",
    "analyze": "vite build --mode production && vite-bundle-visualizer",
    "generate:icons": "node scripts/generate-icons.js",
    "generate:splash": "node scripts/generate-splash.js",
    "lighthouse": "lighthouse http://localhost:4173 --view --preset=desktop --throttling.cpuSlowdownMultiplier=1"
  },
  "dependencies": {
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "vite": "^7.0.4",
    "vite-plugin-pwa": "^1.0.1"
  }
}
