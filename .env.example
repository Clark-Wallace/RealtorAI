# RealtorAI Environment Configuration
# Copy this file to .env.local and fill in your values

# ========================================
# Core Configuration
# ========================================
VITE_API_BASE_URL=https://api.yourdomain.com/v1
VITE_ENVIRONMENT=development

# ========================================
# Real Estate APIs (License Required)
# ========================================
# MLS/IDX Integration (requires real estate license)
REACT_APP_MLS_API_KEY=
REACT_APP_MLS_CLIENT_ID=
REACT_APP_MLS_CLIENT_SECRET=
REACT_APP_MLS_ENDPOINT=https://api.mlsgrid.com

# Zillow (partner access only)
REACT_APP_ZILLOW_ZWS_ID=
REACT_APP_ZILLOW_API_KEY=

# Public Records (various providers)
REACT_APP_PUBLIC_RECORDS_API_KEY=

# ========================================
# Alternative APIs (No License Required)
# ========================================
# Realty Mole via RapidAPI
REACT_APP_RAPIDAPI_KEY=
REACT_APP_REALTY_MOLE_HOST=realty-mole-property-api.p.rapidapi.com

# Rentals.com API (free)
REACT_APP_RENTALS_API_KEY=

# Rentberry API
REACT_APP_RENTBERRY_API_KEY=

# ========================================
# Public Data APIs (Free)
# ========================================
# US Census Bureau (free, instant)
REACT_APP_CENSUS_API_KEY=

# Federal Reserve Economic Data (free, instant)
REACT_APP_FRED_API_KEY=

# Walk Score API (free tier available)
REACT_APP_WALK_SCORE_KEY=

# OpenWeather API (free tier)
REACT_APP_OPENWEATHER_API_KEY=

# ========================================
# Google Services
# ========================================
VITE_GOOGLE_PLACES_API_KEY=
VITE_GOOGLE_MAPS_API_KEY=

# ========================================
# Feature Flags
# ========================================
VITE_ENABLE_PROPERTY_SEARCH=true
VITE_ENABLE_CRM_INTEGRATION=true
VITE_ENABLE_PREFERENCE_TRACKING=true
VITE_ENABLE_SMART_MATCHING=true
VITE_ENABLE_MARKET_INTELLIGENCE=true
VITE_ENABLE_VOICE_FEATURES=false
VITE_ENABLE_OFFLINE_MODE=true

# ========================================
# Analytics & Monitoring (Optional)
# ========================================
# Google Analytics
VITE_GA_TRACKING_ID=

# Sentry Error Tracking
VITE_SENTRY_DSN=

# Mixpanel Product Analytics
VITE_MIXPANEL_TOKEN=

# ========================================
# CRM Integrations (Optional)
# ========================================
# Salesforce
VITE_SALESFORCE_CLIENT_ID=
VITE_SALESFORCE_CLIENT_SECRET=
VITE_SALESFORCE_INSTANCE_URL=

# Follow Up Boss
VITE_FUB_API_KEY=

# kvCORE
VITE_KVCORE_API_KEY=
VITE_KVCORE_TEAM_ID=

# ========================================
# Development Settings
# ========================================
# Mock data settings
VITE_USE_MOCK_DATA=false
VITE_MOCK_DELAY_MS=1000

# API Rate Limiting (requests per minute)
VITE_API_RATE_LIMIT=60

# Cache TTL (seconds)
VITE_CACHE_TTL_LISTINGS=300
VITE_CACHE_TTL_PROPERTY=3600
VITE_CACHE_TTL_MARKET=1800
VITE_CACHE_TTL_DEMOGRAPHICS=86400

# ========================================
# PWA Configuration
# ========================================
VITE_PWA_NAME=RealtorAI
VITE_PWA_SHORT_NAME=RealtorAI
VITE_PWA_THEME_COLOR=#3B82F6
VITE_PWA_BACKGROUND_COLOR=#ffffff

# ========================================
# Security
# ========================================
# Never commit actual values for these!
# Use environment-specific .env files